active class TryInt

  var num : Integer

  def init() : unit
    this.num = 3
  end

  def add(num : Integer) : unit
    this.num.add(num)
    print("New value: {}", this.num)
  end

end

active class Main

  def main(args : [String]) : unit

    var n = 3 : Integer
    var instance = new TryInt()
    instance!add(n)
  end
end
